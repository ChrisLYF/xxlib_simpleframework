<Window x:Class="manage.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:manage"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="480" MinWidth="640">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="303*"/>
            <ColumnDefinition Width="77*"/>
        </Grid.ColumnDefinitions>
        <local:UCLogin HorizontalAlignment="Center" VerticalAlignment="Center" Submit="UCLogin_Submit" />
        <TextBox x:Name="tb_log" Grid.Column="1" IsReadOnly="True" HorizontalAlignment="Stretch" TextWrapping="Wrap" AcceptsReturn="True" Text="" VerticalAlignment="Stretch" />

    </Grid>
</Window>
